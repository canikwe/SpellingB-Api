{
  "moduleFileExtensions": ["js", "json", "ts", "tsx"],
  "rootDir": "..",
  "moduleNameMapper": {
    "^src/(.+)$": "<rootDir>/src/$1",
    "^utils/(.+)$": "<rootDir>/utils/$1"
  },
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(ts|tsx)$": "ts-jest"
  }
}
